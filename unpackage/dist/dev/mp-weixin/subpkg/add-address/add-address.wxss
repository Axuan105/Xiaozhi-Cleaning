page {
  padding: 0;
  background-color: #f5f5f5;
}
.content .content-list {
  background-color: #fff;
}
.content .button2 {
  display: flex;
  align-items: center;
  width: 500rpx;
  margin-top: 50rpx;
  margin-left: 120rpx;
}
.content .button {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  justify-content: space-between;
  margin-top: 50rpx;
}
.content .button .button-a .van-button {
  flex: 1;
  width: 200rpx;
}
.content .button .van-goods-action-button {
  margin-left: 50rpx;
  width: 400rpx;
}
.content .content-list {
  padding: 20rpx 40rpx;
}
.content .content-list .van-cell {
  padding: 20rpx 0;
}
.content .content-list .van-cell .van-cell__title {
  flex: 1;
  width: 170rpx;
}
.content .content-list .content-item {
  display: flex;
  align-items: center;
  height: 60rpx;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #f5f5f5;
}
.content .content-list .content-item .left {
  width: 156rpx;
  margin-right: 50rpx;
  max-width: 180rpx !important;
  color: #646566;
  font-size: 28rpx;
}
.content .content-list .content-item .right {
  flex: 1;
  font-size: 28rpx;
}
.content .van-cell__title {
  max-width: 180rpx !important;
  color: #646566;
  margin-right: 24rpx;
}
.content .default .van-cell__title {
  max-width: 100% !important;
}
.content .city {
  color: #ccc;
  text-align: left;
}
.content .city2 {
  color: #000;
  text-align: left;
}
.content switch {
  transform: scale(0.8);
}
.content .radio {
  display: flex;
  height: 80rpx;
  margin-left: 120rpx;
}
.content .radio .uni-list-cell {
  display: flex;
  align-items: center;
  margin-left: 15rpx;
  transform: scale(0.7);
}