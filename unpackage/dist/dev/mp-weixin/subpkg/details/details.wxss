page {
  padding: 0;
  background-color: #f5f5f5;
}
.nullGg {
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -150rpx;
  margin-left: -150rpx;
}
.nullGg image {
  width: 300rpx;
  height: 300rpx;
}
.nullGg .nullGg-a {
  margin-left: 80rpx;
}
.detail .detail-swiper {
  position: relative;
}
.detail .detail-swiper .swiper {
  z-index: 100;
  height: 620rpx;
}
.detail .detail-swiper .swiper swiper-item view image {
  width: 100%;
  height: 620rpx;
}
.detail .detail-swiper .city {
  padding: 5rpx 20rpx;
  height: 50rpx;
  border-radius: 40rpx;
  position: absolute;
  background: rgba(4, 8, 24, 0.4);
  top: 20rpx;
  left: 29rpx;
  color: #fff;
  font-size: 28rpx;
  line-height: 50rpx;
  font-weight: 600;
}
.detail .detail-swiper .city text {
  margin-left: 5rpx;
}
.detail .detail-swiper .dots {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
}
.detail .detail-swiper .dots .dots-a {
  color: #fff;
  background: rgba(4, 8, 24, 0.4);
  border-radius: 40rpx;
  font-size: 24rpx;
  padding: 5rpx 15rpx;
  position: absolute;
  bottom: 23rpx;
  right: 30rpx;
}
.detail .detail-swiper .dots .active {
  background-color: #d2d2d2;
}
.detail .detail-title {
  background-color: #fff;
