page {
  padding: 0;
  background-color: #f5f5f5;
}
.order {
  background-color: #fff;
}
.item {
  padding: 20rpx;
  display: flex;
  background-color: #fff;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
  padding: 20rpx 30rpx;
}
.item .left .name {
  font-size: 32rpx;
}
.item .left .name:nth-child(2) {
  margin-top: 20rpx;
  font-size: 26rpx;
  color: #000;
}
.item .right {
  padding: 20rpx 0 20rpx 20rpx;
}
.address-border {
  background-color: #f5f5f5;
  background: url(data:image/png;base64,);
  height: 10rpx;
  width: 150%;
  margin-left: -10rpx;
  background-size: auto 100%;
}
.order-shop {
  background-color: #fff;
  padding: 40rpx 0 40rpx 80rpx;
  margin-top: 20rpx;
  display: flex;
}
.order-shop .shop-img image {
  width: 140rpx;
  height: 140rpx;
}
.order-shop .shop-title {
  margin-left: 20rpx;
}
.point-out {
  color: #949ea3;
  font-size: 26rpx;
  margin-top: 40rpx;
  padding: 0 40rpx;
}
.button {
  width: 95%;
  padding: 0 20rpx;
  position: absolute;
  bottom: 30rpx;
}
.button .van-button {
  height: 100rpx;
}